{
  "name": "overvue",
  "version": "2.0.0",
  "description": "Vue Prototype DevTool",
  "productName": "OverVue",
  "cordovaId": "org.cordova.quasar.app",
  "author": "Team OverVue, Team Vuetiful",
  "private": true,
  "bugs": {
    "url": "https://github.com/TeamOverVue/OverVue/issues"
  },
  "homepage": "https://github.com/TeamOverVue/OverVue",
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@quasar/extras": "^1.8.2",
    "aws-amplify": "^3.0.18",
    "aws-amplify-vue": "^2.1.1",
    "aws-appsync": "^3.0.3",
    "fs-extra": "^9.0.1",
    "localforage": "^1.7.4",
    "lodash": ">=4.17.15",
    "lodash.isequal": "^4.5.0",
    "lodash.throttle": "^4.1.1",
    "mousetrap": "^1.6.5",
    "npm": "^6.14.5",
    "prismjs": "^1.20.0",
    "quasar": "^1.12.6",
    "vue-custom-context-menu": "^3.0.2",
    "vue-drag-resize": "^1.3.2",
    "vue-draggable-nested-tree": "^2.2.20",
    "vue-draggable-resizable": "^2.2.0",
    "vue-loader": "^15.9.2",
    "vue-multiselect": "^2.1.6",
    "vue-prism-editor": "^0.6.1",
    "vued3tree": "^5.1.0",
    "vuedraggable": "^2.23.2",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@quasar/app": "^2.0.0",
    "@quasar/quasar-app-extension-testing": "^1.0.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "^1.0.1",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "devtron": "^1.4.0",
    "electron": "^9.0.4",
    "electron-builder": "^22.7.0",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.0.0",
    "eslint": "^7.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^6.2.2",
    "strip-ansi": "^6.0.0",
    "vue-cli-plugin-electron-builder": "^1.4.6"
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "last 1 version, not dead, ie >= 11"
  ]
}
